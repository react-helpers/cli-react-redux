{
  "name": "cli-react-redux",
  "version": "1.3.2",
  "description": "React Redux CLI",
  "main": "index.js",
  "scripts": {
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/react-helpers/cli-react-redux"
  },
  "keywords": [
    "React",
    "Redux",
    "CLI",
    "create-react-app",
    "code generation",
    "unit tests generation",
    "e2e tests generation",
    "Sagas",
    "Reselect"
  ],
  "author": "sandermets0@gmail.com",
  "contributors": [
    "Kaspar Vihm <kaspar.vihm@gmail.com> (https://github.com/kasparvihm)"
  ],
  "license": "MIT",
  "bin": {
    "cli-react-redux": "./bin/cli-react-redux"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "bugs": {
    "url": "https://github.com/react-helpers/cli-react-redux/issues"
  },
  "homepage": "https://github.com/react-helpers/cli-react-redux#readme",
  "dependencies": {
    "fs-extra": "^5.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.2",
    "eslint": "^4.12.1",
    "eslint-plugin-node": "^5.2.1",
    "jest": "^20.0.4",
    "jest-environment-node": "^20.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}",
      "!src/index.js",
      "!src/i18n/index.js",
      "!src/registerServiceWorker.js",
      "!src/config.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": -10
      }
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  }
}
